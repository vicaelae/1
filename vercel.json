{
  "rewrites": [
    {
      "source": "/google/:path*",
      "destination": "https://www.google.com/:path*"
    },
    {
      "source": "/wiki/:path*",
      "destination": "https://en.wikipedia.org/:path*"
    }
  ],
  "headers": [
    {
      "source": "/google/:path*",
      "headers": [
        {
          "key": "host",
          "value": "www.google.com"
        }
      ]
    },
    {
      "source": "/wiki/:path*",
      "headers": [
        {
          "key": "host",
          "value": "en.wikipedia.org"
        }
      ]
    }
  ]
}
