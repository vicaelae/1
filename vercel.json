{
    "rewrites": [
        {
            "source": "/google/:path*",
            "destination": "https://www.google.com/:path*",
            "headers": {
                "host": "www.google.com"
            }
        },
        {
            "source": "/wiki/:path*",
            "destination": "https://zh.wikipedia.org//:path*",
            "headers": {
                "host": "zh.wikipedia.org"
            }
        }
    ]
}
